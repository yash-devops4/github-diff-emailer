# AWS SES SMTP Configuration
SMTP_SERVER=email-smtp.us-east-1.amazonaws.com
SMTP_PORT=587
SMTP_USERNAME=your_aws_ses_smtp_username
SMTP_PASSWORD=your_aws_ses_smtp_password

# Email Settings
FROM_EMAIL=notifications@yourcompany.com
TO_EMAIL=team@yourcompany.com

# GitHub Configuration
GITHUB_TOKEN=ghp_your_github_personal_access_token_here

# Webhook Security (generate with: openssl rand -hex 32)
WEBHOOK_SECRET=generate_random_string_here

# Server Configuration
PORT=5000
